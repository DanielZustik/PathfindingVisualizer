<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<BorderPane fx:controller="com.pathfindingvisualizer.Controller"
            xmlns:fx="http://javafx.com/fxml">
    <bottom>
        <HBox spacing="10">
            <Button fx:id="startBtn" onAction="#handleStartBtnAction" text="Start" prefWidth="90"/>
            <Button text="Stop" prefWidth="90"/>
            <Button fx:id="generateBtn" onAction="#handleGenerateBtnAction" text="Generate" prefWidth="90"/>

        </HBox>
    </bottom>
    <center>
        <Pane fx:id="gridPlaceholder"/>
    </center>
</BorderPane>